    genre        ( biography | fantasy )     'fantasy'
    category     ( fiction | nonfiction )    'fiction'
    pages        CDATA                     #REQUIRED
    price        CDATA                     #REQUIRED
    payment      ( cash | credit | none )    'none'
>
